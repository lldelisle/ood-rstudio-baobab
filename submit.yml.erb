---
batch_connect:
  template: "basic"
  conn_params:
    - csrf_token  
script:
  native:
    - "--ntasks"
    - "1"
    - "--cpus-per-task"
    - "<%= cpus %>"
    - "--mem"
    - "<%= mem_gb %>G"
    - "--partition"
    - "<%= auto_queues %>"
    - "--time"
    - "<%= time %>"
    - "--signal"
    - "USR2"
